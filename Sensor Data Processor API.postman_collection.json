		{
			"name": "Upload",
			"item": [
				{
					"name": "Upload CSV File",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// Save uploadId for subsequent requests",
									"if (pm.response.code === 202) {",
									"    var jsonData = pm.response.json();",
									"    pm.environment.set('uploadId', jsonData.data.uploadId);",
									"    pm.test('Upload successful', function () {",
									"        pm.expect(jsonData.success).to.be.true;",
									"    });",
									"}"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [],
						"body": {
							"mode": "formdata",
							"formdata": [
								{
									"key": "file",
									"description": "CSV file containing sensor data",
									"type": "file",
									"src": "sensor_data.csv"
								}
							]
						},
						"url": "{{base_url}}/api/upload",
						"description": "Upload a CSV file for processing. Returns uploadId immediately."
					},
					"response": []
				}
			]
		},
